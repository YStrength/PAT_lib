int prime ( int p )
{
	int k,ret=1;
	if (p<=1){
			ret=0;
		}
	for(k=2;k<p-1;k++){
		if(p%k==0){
			ret =0;
			break;
		} 
		else {
			ret =1;
		}
	}
return ret;
}

int PrimeSum ( int m, int n )
{
	int i,sum=0;
	for(i=m;i<=n;i++){
		if (prime(i)){
			sum +=i;
		}	
	}
return sum;
}

